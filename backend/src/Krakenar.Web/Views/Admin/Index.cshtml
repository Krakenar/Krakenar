@inject Krakenar.Web.Settings.AdminSettings Admin

<!DOCTYPE html>
<html lang="en">

<head>
  <meta charset="UTF-8" />
  <link rel="icon" href="~/favicon.ico" />
  <meta name="viewport" content="width=device-width, initial-scale=1.0" />
  <title>Krakenar</title>
  <link rel="stylesheet" href="~/assets/index.css">
</head>
<body>
  <div id="krakenar"></div>
  <script>
    window.KRAKENAR_BASE_URL = "@(Admin.BasePath)";
    window.KRAKENAR_ENABLE_SWAGGER = @(Admin.EnableSwagger.ToString().ToLowerInvariant());
    window.KRAKENAR_VERSION = "@(Admin.Version)";
  </script>
  <script type="module" crossorigin src="~/assets/index.js"></script>
</body>
</html>
